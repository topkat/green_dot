{
  "name": "$$projectName",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "license": "MIT",
  "packageManager": "yarn@1.22.19",
  "scripts": {
    "==--->     START     <---==": "",
    "dev": "green_dot dev",
    "start": "green_dot start",
    "start:test": "NODE_ENV=test green_dot dev",
    "start:dev": "green_dot start",
    "start:prod": "NODE_ENV=production green_dot start",
    "==--->    PUBLISH    <---==": "",
    "publishSdks": "green_dot publishSdks",
    "==--->     BUILD     <---==": "",
    "build": "green_dot build",
    "build:prod": "NODE_ENV=production green_dot build",
    "==--->    GENERATE   <---==": "",
    "generate": "green_dot generate",
    "==--->      TEST     <---==": "",
    "test": "jest --ci --runInBand --maxConcurrency=1 --no-clean",
    "test:api": "green_dot test",
    "start:backend-ci": "concurrently -k --success=first --kill-others-on-fail \"NODE_ENV=test green_dot start\" \"sh -c 'sleep 5 && green_dot test --ci'\"",
    "coverage": "bun ./coverage.ts",
    "==--->     CLEAN     <---==": "",
    "clean": "(find . -type d -name dist -not -path '*/node_modules/*' -exec rm -rf {} \\; || true) && (find . -type d -name node_modules -prune -exec rm -rf {} \\; || true)",
    "==--->     DEBUG     <---==": "",
    "// Replaces a node_modules green_dot version with local one. Please change the path accordingly)": "",
    "symlink": "cd ./node_modules/ && rm -rf ./green_dot && ln -s ../../modules/green_dot ./green_dot && cd ./.bin && rm -f ./green_dot && ln -s ../../../modules/green_dot/src/cli/cliEntryPoint.ts ./green_dot || echo 'IF IT HAS PERMISSION ERROR PLEASE RUN chmod +x $(realpath ./node_modules/.bin/dot)' && rm -f ./gdStartProd && ln -s ../../../modules/green_dot/dist/src/cli/startProdSpecialEntryPoint.js ./gdStartProd",
    "rmGdFolder": "cd ./node_modules/ && rm -rf ./green_dot"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "crypto-js": "^4.2.0",
    "green_dot": "$$gdVersion",
    "js-sha256": "^0.11.0",
    "jsonwebtoken": "^9.0.2",
    "topkat-utils": "^1.3.17",
    "typescript": "^5.3.3",
    "typescript-generic-types": "^1.0.12"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "^22.13.13",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@tanstack/react-query": "^5.80.6",
    "@typescript-eslint/parser": "^6.6.0",
    "bun": "^1.2.10",
    "concurrently": "^9.1.2",
    "eslint": "^8.56.0",
    "istanbul": "^0.4.5",
    "istanbul-merge": "^2.0.0",
    "jest": "^29.6.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "react": "^19.1.0"
  }
}